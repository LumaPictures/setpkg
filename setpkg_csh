


setenv PATH ${PATH}:`dirname $SETPKG_PATH`
#alias addenv 'eval `$SETPKG_PATH/addenv.py \!*`'
#alias delenv 'eval `$SETPKG_PATH/delenv.py \!*`'
alias setpkg 'eval `setpkg.py --pid $$ set $SHELL \!*`' 
alias unsetpkg 'eval `setpkg.py --pid $$ unset $SHELL \!*`' 
alias lspkg 'setpkg.py --pid $$ list \!*' 
alias runpkg 'setpkg \!*; eval `setpkg.py info --exe \!*`'

# setup aliases
eval `setpkg.py alias $SHELL`
